{
  "name": "rclean",
	"displayName": "Rclean",
	"description": "Clean React project",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.61.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:rclean.cleanProjectArchitecture",
        "onCommand:rclean.addTailwindcss"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
            "command": "rclean.cleanProjectArchitecture",
            "title": "Clean My Project Architecture "
			},
			{
            "command": "rclean.addTailwindcss",
            "title": "Add Tailwindcss To My Project"
			}
	]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
        "@types/vscode": "^1.61.0",
        "@types/glob": "^7.1.4",
        "@types/mocha": "^9.0.0",
        "@types/node": "14.x",
        "eslint": "^7.32.0",
        "glob": "^7.1.7",
        "mocha": "^9.1.1",
        "typescript": "^4.4.3",
        "@vscode/test-electron": "^1.6.2"
    }

}
